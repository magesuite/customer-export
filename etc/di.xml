<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="export_customer_ftp" xsi:type="object">MageSuite\CustomerExport\Console\Command\Export</item>
            </argument>
        </arguments>
    </type>
    <type name="MageSuite\CustomerExport\Services\Parser\ParserFactory">
        <arguments>
            <argument name="classMappings" xsi:type="array">
                <item name="xml" xsi:type="object">MageSuite\CustomerExport\Services\Parser\XmlParser</item>
            </argument>
        </arguments>
    </type>
    <type name="MageSuite\CustomerExport\Services\File\WriterFactory">
        <arguments>
            <argument name="classMappings" xsi:type="array">
                <item name="file" xsi:type="object">MageSuite\CustomerExport\Services\File\Adapter\FileAdapter</item>
                <item name="ftp" xsi:type="object">MageSuite\CustomerExport\Services\File\Adapter\FTPAdapter</item>
            </argument>
        </arguments>
    </type>
</config>
